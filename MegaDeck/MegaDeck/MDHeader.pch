#ifndef MegaDeck_MDHeader_h
#define MegaDeck_MDHeader_h

#define AssertWithStackSymbols(x) \
do { \
if (!(x)) { \
NSLog (@"%s failed assertion\n%@", #x, [NSThread callStackSymbols]); \
abort(); \
} \
} while(0)

#endif
